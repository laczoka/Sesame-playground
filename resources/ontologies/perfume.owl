  xmlns:skos="http://www.w3.org/2004/02/skos/core#"
>
  <prfm:PerfumeType rdf:about="http://purl.org/opdm/perfume#EspritDeParfum">
    <rdfs:comment xml:lang="en">Class of perfumes with high perfume oil concentration</rdfs:comment>
    <rdfs:label xml:lang="en">Esprit de Parfum</rdfs:label>
