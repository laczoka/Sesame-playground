  <gr:QuantitativeValue rdf:about="http://purl.org/opdm/perfume#FiftyML">
    <gr:hasUnitOfMeasurement rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MLT</gr:hasUnitOfMeasurement>
