    <rdfs:domain rdf:resource="http://purl.org/opdm/perfume#Perfume"/>
    <rdfs:label xml:lang="en">Heart note</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/goodrelations/v1#qualitativeProductOrServiceProperty"/>
